{
  "compilerOptions": {
    "allowJs": true,
    "declaration": true,
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "sourceMap": true,
    "outDir": "./dist",
    "target": "es5"
  },
  "include": ["src/**/*"],
  "exclude": ["src/**/*.spec.ts"],
}
