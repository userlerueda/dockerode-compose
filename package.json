{
  "name": "dockerode-compose",
  "version": "1.4.0",
  "description": "docker-compose in nodejs using dockerode",
  "main": "./dist/bundle.cjs",
  "module": "./dist/bundle.mjs",
  "exports": {
    "require": "./dist/bundle.cjs",
    "import": "./dist/bundle.mjs",
    "default": "./dist/bundle.mjs"
  },
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "test": "npx mocha -r ts-node/register src/**/*.spec.ts",
    "test:watch": "npx mocha -w -r ts-node/register src/**/*.spec.ts",
    "build": "tsc --build && tsc --build tsconfig.cjs.json && npx webpack",
    "clean": "rm -rf dist && rm -rf lib-esm && rm -rf lib",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apocas/dockerode-compose.git"
  },
  "author": "Pedro Dias <petermdias@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/apocas/dockerode-compose/issues"
  },
  "keywords": [
    "docker",
    "docker-compose"
  ],
  "homepage": "https://github.com/apocas/dockerode-compose#readme",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "dockerode": "^3.3.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "tar-fs": "^2.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/dockerode": "^3.3.8",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.181",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@types/tar-fs": "^2.0.1",
    "chai": "~4.3.6",
    "memorystream": "~0.3.1",
    "mocha": "^9.2.2",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "typescript-declaration-webpack-plugin": "^0.2.2",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
